{
  "expo": {
    "name": "Premeditated",
    "version": "1.0.11",
    "description": "Simple meditation timers.   Save your favorites.",
    "slug": "premeditated",
    "privacy": "public",
    "sdkVersion": "25.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": { "package": "com.williamskbw.premeditated" },
    "packagerOpts": {
      "assetExts": ["ttf"]
    },
    "assetBundlePatterns": [
      "assets/sound/*",
      "assets/sound/background/*",
      "assets/backgrounds/*",
      "node_modules/@expo/vector-icons/fonts"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
